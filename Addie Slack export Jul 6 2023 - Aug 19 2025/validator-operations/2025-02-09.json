            "team": "E014GLQD2NN",
            "name": "stasge",
            "is_restricted": false,
            "is_ultra_restricted": false
        },
        "thread_ts": "1739166257.280329",
        "parent_user_id": "U01DARYACCT",
        "blocks": [
            {
                "type": "rich_text",
                "block_id": "4hKRe",
                "elements": [
                    {
                        "type": "rich_text_section",
